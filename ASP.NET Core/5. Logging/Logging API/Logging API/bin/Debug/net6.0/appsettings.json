{
  "Logging": {
    "Console": {
      "LogLevel": {
        "Default": "Information",
        "Microsoft.AspNetCore": "Warning"
      },
      "FormatterName": "json",
      "FormatterOptions": {
        "TimestampFormat": "[HH:mm:ss] ",
        "JsonWriterOptions": {
          "Indented": true
        }
      }
      //"FormatterName": "simple",
      //"FormatterOptions": {
      //  "TimestampFormat": "[HH:mm:ss] "
      //}
    }
  },
  "AllowedHosts": "*"
}
